    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@google-cloud/recaptcha-enterprise": "^6.0.1",
    "@heroicons/react": "^2.2.0",
    "@vapi-ai/web": "^2.2.5",
    "antd": "^5.24.6",
    "geist": "^1.3.1",
    "next": "^15.3.0",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.0",
    "react": "^18.2.0",
    "react-chat-elements": "^12.0.18",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-slick": "^0.30.3",
    "sharp": "^0.34.1",
    "slick-carousel": "^1.8.1",
    "tailwindcss-theme": "^1.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
