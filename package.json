    "@types/slick-carousel": "^1.6.40",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.5.3",
    "sass": "^1.86.3",
    "tailwindcss": "^4.1.3",
    "typescript": "^5"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": [
    "last 1 version",
    "not dead",
    "not < 2%",
    "not ie <= 11"
  ],
  "overrides": [
    {
      "files": [
        "**/__tests__/**/*.[jt]s?(x)",
        "**/?(*.)+(spec|test).[jt]s?(x)"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}
